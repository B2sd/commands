      // Category mappings
      const categoryMappings = {
          fortnite: ['!tracker', '!даблмувмент', '!яркость', '!s', '!бинды', '!sigma', '!sense', '!edit', '!цветокор', '!resolution', '!ыуеештпы', '!сигма', '!sensitivity', '!doublemovement', '!sens', '!colorblind', '!дельт', '!res', '!трекер', '!hz', '!крейзи', '!супер', '!movement', '!dpi', '!карта', '!editonrelease', '!1в1', '!дпи', '!сенса', '!мувмент', '!настройки', '!hud', '!brightness', '!ванвиван', '!map', '!дипиай', '!graphic', '!код', '!1v1'],
          setup: ['!н', '!вебка', '!микрофон', '!девайсы', '!mousepad', '!ьщгыу', '!setup', '!headset', '!клава', '!кейборд', '!микро', '!мышь', '!pcboost', '!phone', '!pc', '!micro', '!naushniki', '!maus', '!писи', '!microphone', '!nayshnikinaangliyskom', '!кейбоард', '!headphones', '!к', '!м', '!монитор', '!наушникинаанглийском', '!наушники', '!пк', '!буст', '!devices', '!keyboard', '!уши', '!лунищфкв', '!monitor', '!мышка', '!mouse', '!mic'],
          socials: ['!тт', '!тг', '!ds', '!д', '!tiktok', '!donate', '!спотифай', '!discord', '!ют', '!tg', '!shorts', '!лицо', '!onlyfans', '!заказатьтрек', '!x', '!дс', '!youtube', '!хомяк', '!playlist', '!соцсети', '!дискорд', '!музыка', '!тикток', '!плейлист', '!inst', '!донат', '!трейд', '!плэйлист', '!spotify', '!twitter', '!yt', '!криптовалюта', '!аук', '!ютуб', '!7tv']
      };

      // УЛУЧШЕННАЯ СИСТЕМА АНИМИРОВАННЫХ ЧАСТИЦ
      class ParticleSystem {
          constructor() {
              this.container = document.querySelector('.cyber-background');
              this.particles = [];
              this.init();
          }

          init() {
              this.createParticles();
              setInterval(() => this.createParticle(), 1500);
          }

          createParticles() {
              for (let i = 0; i < 8; i++) {
                  setTimeout(() => this.createParticle(), i * 200);
              }
          }

          createParticle() {
              if (this.particles.length >= 12) return;

              const particle = document.createElement('div');
              particle.className = 'particle';
              
              const left = Math.random() * window.innerWidth;
              particle.style.left = left + 'px';
              particle.style.animationDuration = (Math.random() * 3 + 4) + 's';
              particle.style.animationDelay = Math.random() * 2 + 's';

              this.container.appendChild(particle);
              this.particles.push(particle);

              setTimeout(() => {
                  if (particle.parentNode) {
                      this.removeParticle(particle);
                  }
              }, 8000);
          }

          removeParticle(particle) {
              const index = this.particles.indexOf(particle);
              if (index > -1) {
                  this.particles.splice(index, 1);
              }
              if (particle.parentNode) {
                  particle.parentNode.removeChild(particle);
              }
          }
      }

      // УЛУЧШЕННЫЙ КЛАСС ПРИЛОЖЕНИЯ КОМАНД С ПЛАВНОЙ ПРОКРУТКОЙ И АНИМАЦИЯМИ
      class CommandsApp {
          constructor() {
              this.commands = [];
              this.filteredCommands = [];
              this.currentCategory = 'all';
              this.observer = null;
              this.init();
          }

          init() {
              this.parseCommands();
              this.renderCommands();
              this.setupEventListeners();
              this.setupIntersectionObserver();
              this.setupSmoothScrolling();
              this.generateAllSpamCommands(); // Call this to generate the new section
          }

          parseCommands() {
              const rows = rawCommands.trim().split('\n');
              this.commands = rows.map(row => {
                  const match = row.match(/^!(.*?)\s*-\s*"(.*?)"$/);
                  if (match) {
                      const command = '!' + match[1].trim();
                      return {
                          command: command,
                          response: match[2].trim(),
                          category: this.getCommandCategory(command)
                      };
                  }
                  return null;
              }).filter(Boolean);
              
              this.filteredCommands = [...this.commands];
          }

          getCommandCategory(command) {
              for (const [category, commands] of Object.entries(categoryMappings)) {
                  if (commands.includes(command)) {
                      return category;
                  }
              }
              return 'other';
          }

          makeLinksClickable(text) {
              return text.replace(/(https?:\/\/[^\s]+)/g, '<a href="$1" target="_blank">$1</a>');
          }

          setupSmoothScrolling() {
              const style = document.createElement('style');
              style.textContent = `
                  * {
                      scroll-behavior: smooth;
                  }
                  
                  .commands-list {
                      scroll-behavior: smooth;
                      -webkit-overflow-scrolling: touch;
                  }
              `;
              document.head.appendChild(style);
          }

          setupIntersectionObserver() {
              this.observer = new IntersectionObserver((entries) => {
                  entries.forEach(entry => {
                      if (entry.isIntersecting) {
                          setTimeout(() => {
                              entry.target.classList.add('visible');
                          }, Math.random() * 100);
                      }
                  });
              }, {
                  threshold: 0.1,
                  rootMargin: '50px 0px 50px 0px'
              });
          }

          renderCommands() {
              const container = document.getElementById('commandsList');
              
              if (this.filteredCommands.length === 0) {
                  container.innerHTML = '<div class="no-results">Команды не найдены</div>';
                  return;
              }

              // Очищаем предыдущий observer
              if (this.observer) {
                  this.observer.disconnect();
              }

              // Плавное исчезновение старого контента
              container.style.opacity = '0.3';
              container.style.transform = 'translateY(10px)';

              setTimeout(() => {
                  const fragment = document.createDocumentFragment();
                  
                  this.filteredCommands.forEach((cmd, index) => {
                      const row = document.createElement('div');
                      row.className = 'command-row';
                      row.dataset.index = index;
                      row.innerHTML = `
                          <div class="command-name">
                              <span>${cmd.command}</span>
                              <button class="copy-btn" onclick="app.copyCommand('${cmd.command}')" title="Копировать команду">
                                  <svg class="copy-icon" viewBox="0 0 24 24">
                                      <path d="M16 1H4c-1.1 0-2 .9-2 2v14h2V3h12V1zm3 4H8c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h11c1.1 0 2-.9 2-2V7c0-1.1-.9-2-2-2zm0 16H8V7h11v14z"/>
                                  </svg>
                              </button>
                          </div>
                          <div class="command-response">${this.makeLinksClickable(cmd.response)}</div>
                      `;
                      fragment.appendChild(row);
                  });

                  container.innerHTML = '';
                  container.appendChild(fragment);

                  // Плавное появление нового контента
                  container.style.opacity = '1';
                  container.style.transform = 'translateY(0)';

                  // Настраиваем observer для новых элементов
                  this.setupIntersectionObserver();
                  const commandRows = container.querySelectorAll('.command-row');
                  commandRows.forEach(row => {
                      this.observer.observe(row);
                  });

                  // Показываем первые элементы с задержкой
                  commandRows.forEach((row, index) => {
                      if (index < 6) {
                          setTimeout(() => {
                              row.classList.add('visible');
                          }, index * 60);
                      }
                  });
              }, 200);
          }

          filterByCategory(category) {
              this.currentCategory = category;
              
              // Update category buttons
              document.querySelectorAll('.category-btn').forEach(btn => {
                  btn.classList.remove('active');
              });
              document.querySelector(`[data-category="${category}"]`).classList.add('active');
              
              // Filter commands
              if (category === 'all') {
                  this.filteredCommands = [...this.commands];
              } else {
                  this.filteredCommands = this.commands.filter(cmd => cmd.category === category);
              }
              
              // Apply search if there's a search term
              const searchTerm = document.getElementById('commandSearch').value.toLowerCase();
              if (searchTerm) {
                  this.filterCommands(searchTerm);
              } else {
                  this.renderCommands();
              }
          }

          filterCommands(searchTerm) {
              const term = searchTerm.toLowerCase();
              let baseCommands = this.currentCategory === 'all' ? this.commands : 
                  this.commands.filter(cmd => cmd.category === this.currentCategory);
              
              this.filteredCommands = baseCommands.filter(cmd => 
                  cmd.command.toLowerCase().includes(term) || 
                  cmd.response.toLowerCase().includes(term)
              );
              this.renderCommands();
          }

          copyCommand(command) {
              navigator.clipboard.writeText(command).then(() => {
                  this.showToast();
              }).catch(err => {
                  console.error('Ошибка копирования:', err);
              });
          }

          showToast() {
              const toast = document.getElementById('toast');
              toast.classList.add('show');
              setTimeout(() => {
                  toast.classList.remove('show');
              }, 3000);
          }

          setupEventListeners() {
              const searchInput = document.getElementById('commandSearch');
              let debounceTimer;

              searchInput.addEventListener('input', (e) => {
                  clearTimeout(debounceTimer);
                  debounceTimer = setTimeout(() => {
                      this.filterCommands(e.target.value);
                  }, 100);
              });

              // Category buttons
              document.querySelectorAll('.category-btn').forEach(btn => {
                  btn.addEventListener('click', () => {
                      this.filterByCategory(btn.dataset.category);
                  });
              });

              // Keyboard shortcuts
              document.addEventListener('keydown', (e) => {
                  if ((e.metaKey || e.ctrlKey) && e.key === 'k') {
                      e.preventDefault();
                      searchInput.focus();
                  }
                  if (e.key === 'Escape') {
                      searchInput.blur();
                      searchInput.value = '';
                      this.filterCommands('');
                  }
              });
          }

          generateAllSpamCommands() {
              const container = document.getElementById('allSpamCommandsContainer');
              const columnsData = [
                  ['!карта', '!карта2', '!карта3', '!карта4', '!карта5', '!карта6', '!карта7', '!карта8', '!карта9', '!карта10', '!карта2а', '!карта3а', '!карта4а', '!карта5а'],
                  ['!крейзипвп', '!крейзипвп2', '!крейзипвп3', '!крейзипвп4', '!крейзипвп5', '!крейзипвп6', '!крейзипвп7', '!крейзипвп8', '!крейзипвп9', '!крейзипвп10', '!крейзипвп2а', '!крейзипвп3а', '!крейзипвп4а', '!крейзипвп5а'],
                  ['!аук', '!аук2', '!аук3', '!аук4', '!аук5', '!аук6', '!аук7', '!аук8', '!аук9', '!аук10', '!аук2а', '!аук3а', '!аук4а', '!аук5а'],
                  ['!код', '!код2', '!код3', '!код4', '!код5', '!код6', '!код7', '!код8', '!код9', '!код10', '!код2а', '!код3а', '!код4а', '!код5а'],
                  ['!музыка', '!музыка2', '!музыка3', '!музыка4', '!музыка5', '!музыка6', '!музыка7', '!музыка8', '!музыка9', '!музыка10', '!музыка2а', '!музыка3а', '!музыка4а', '!музыка5а'],
                  ['!сигматг', '!сигматг2', '!сигматг3', '!сигматг4', '!сигматг5', '!сигматг6', '!сигматг7', '!сигматг8', '!сигматг9', '!сигматг10', '!сигматг2а', '!сигматг3а', '!сигматг4а', '!сигматг5а'],
                  ['!соц', '!соц2', '!соц3', '!соц4', '!соц5', '!соц6', '!соц7', '!соц8', '!соц9', '!соц10', '!соц2а', '!соц3а', '!соц4а', '!соц5а'],
                  ['!тг', '!тг2', '!тг3', '!тг4', '!тг5', '!тг6', '!тг7', '!тг8', '!тг9', '!тг10', '!тг2а', '!тг3а', '!тг4а', '!тг5а'],
                  ['!дс', '!дс2', '!дс3', '!дс4', '!дс5', '!дс6', '!дс7', '!дс8', '!дс9', '!дс10', '!дс2а', '!дс3а', '!дс4а', '!дс5а'],
                  ['!игруля', '!игруля2', '!игруля3', '!игруля4', '!игруля5', '!игруля6', '!игруля7', '!игруля8', '!игруля9', '!игруля10', '!игруля2а', '!игруля3а', '!игруля4а', '!игруля5а'],
                  ['!крейзи', '!крейзи2', '!крейзи3', '!крейзи4', '!крейзи5', '!крейзи6', '!крейзи7', '!крейзи8', '!крейзи9', '!крейзи10'],
                  ['!сигма', '!сигма2', '!сигма3', '!сигма4', '!сигма5', '!сигма6', '!сигма7', '!сигма8', '!сигма9', '!сигма10']
              ];

              columnsData.forEach(columnCommands => {
                  const columnDiv = document.createElement('div');
                  columnDiv.className = 'command-column';
                  columnCommands.forEach(cmd => {
                      const commandSpan = document.createElement('span');
                      commandSpan.className = 'copy-command-item';
                      commandSpan.textContent = cmd;
                      commandSpan.onclick = () => this.copyCommand(cmd);
                      columnDiv.appendChild(commandSpan);
                  });
                  container.appendChild(columnDiv);
              });
          }
      }

      // Navigation functions
      function showModGuide() {
          document.getElementById('mainPage').style.display = 'none';
          document.getElementById('modPage').classList.add('active');
          document.body.style.overflow = 'hidden'; /* ADDED: Hide main page scrollbar */
          document.getElementById('modPage').style.overflowY = 'hidden'; /* ADDED: Hide purple scrollbar for categories */
          showModCategories();
      }

      function showMainPage() {
          document.getElementById('modPage').classList.remove('active');
          document.getElementById('mainPage').style.display = 'block';
          document.body.style.overflow = ''; /* ADDED: Restore main page scrollbar */
          document.getElementById('modPage').style.overflowY = 'hidden'; /* Ensure mod guide scrollbar is hidden when leaving */
      }

      function showModCategories() {
          document.querySelectorAll('.mod-content').forEach(content => {
              content.classList.remove('active');
          });
          document.getElementById('modCategories').style.display = 'grid';
          document.getElementById('modPage').style.overflowY = 'hidden'; /* ADDED: Hide purple scrollbar for categories view */
      }

      function showModContent(section) {
          document.getElementById('modCategories').style.display = 'none';
          document.querySelectorAll('.mod-content').forEach(content => {
              content.classList.remove('active');
          });
          document.getElementById(section + 'Content').classList.add('active');
          document.getElementById('modPage').style.overflowY = 'auto'; /* ADDED: Show purple scrollbar for content view */
      }

      // Spam commands filter function
      function filterSpamCommands(type) {
          // Update filter buttons
          document.querySelectorAll('.filter-btn').forEach(btn => {
              btn.classList.remove('active');
          });
          event.target.classList.add('active');

          const bothCommands = document.getElementById('bothCommands');
          const normalCommands = document.getElementById('normalCommands');
          const bothTitle = bothCommands.previousElementSibling;
          const normalTitle = normalCommands.previousElementSibling;

          switch(type) {
              case 'all':
                  bothTitle.style.display = 'block';
                  bothCommands.style.display = 'grid';
                  normalTitle.style.display = 'block';
                  normalCommands.style.display = 'grid';
                  break;
              case 'both':
                  bothTitle.style.display = 'block';
                  bothCommands.style.display = 'grid';
                  normalTitle.style.display = 'none';
                  normalCommands.style.display = 'none';
                  break;
              case 'normal':
                  bothTitle.style.display = 'none';
                  bothCommands.style.display = 'none';
                  normalTitle.style.display = 'block';
                  normalCommands.style.display = 'grid';
                  break;
          }
      }

      // Toggle function for the new "Show all spam commands" section
      function toggleAllSpamCommands() {
          const container = document.getElementById('allSpamCommandsContainer');
          const button = document.getElementById('toggleAllSpamCommandsBtn');
          if (container.style.display === 'none' || container.style.display === '') {
              container.style.display = 'grid'; // Use grid for the columns
              button.textContent = 'Скрыть все примеры команд:';
          } else {
              container.style.display = 'none';
              button.textContent = 'Показать все примеры команд:';
          }
      }

      // Initialize app and particle system
      const app = new CommandsApp();
      const particleSystem = new ParticleSystem();
  </script>
</body>
</html>
